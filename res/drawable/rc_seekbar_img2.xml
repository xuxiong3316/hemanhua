<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="0.0dip" />
            <gradient android:startColor="#ff777777" android:endColor="#ff777777" android:angle="0.0" android:centerY="2.0" android:centerColor="#ff777777" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="0.0dip" />
                <gradient android:startColor="#ffff6e00" android:endColor="#ffff6e00" android:angle="270.0" android:centerY="2.0" android:centerColor="#ffff6e00" />
            </shape>
        </clip>
    </item>
</layer-list>
